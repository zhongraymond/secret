<form action="/users/" method="post">
</br>
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <table>
  <tr>
<td style="text-align: left"><label for="Email">Email</label></td>
  <td style="text-align: left"><input type="email" name="user[email]"/></td>
  </tr>
  <tr>
  <td style="text-align: left"><label for="Name">Name</label></td>
  <td style="text-align: left"><input type="text" name="user[name]"/></td>
  </tr>
  <tr>
  <td style="text-align: left"><label for="Password">Password</label></td>
  <td style="text-align: left"><input type="password" name="user[password]"/></td>
  </tr>
  <tr>
  <td style="text-align: left"><label for="Password Confirmation">Password Confirmation</label></td>
  <td style="text-align: left"><input type="password" name="user[confirm_password]"/></td>
  </tr>
  <tr>
  <td style="text-align: left"></td>
  <td style="text-align: right"><input type="submit" value="Join" /></td>
  </tr>
</table>

</form>
